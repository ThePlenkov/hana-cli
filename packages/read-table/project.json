{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/read-table/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/read-table",
        "main": "packages/read-table/src/index.ts",
        "tsConfig": "packages/read-table/tsconfig.lib.json",
        "assets": ["packages/read-table/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/read-table/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/read-table"],
      "options": {
        "jestConfig": "packages/read-table/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
